      <item tag="python">result = self.sortedRefExpDims()

import operator

# Get ExpMeasurements in traversal order
steps = self.nmrExpPrototype.sortedExpGraphs()[0].sortedExpSteps()
steps.sort(key=operator.attrgetter('stepNumber'))
stepMeasurements = [x.expMeasurement for x in steps]

if not self.isReversed:
  # start with last in traverse, unless experiment is reversed
  stepMeasurements.reverse()

for measurement in stepMeasurements:
  for expDim in result:
    if expDim.findFirstRefExpDimRef(expMeasurement=measurement) is not None:
      # DImension matches acquisition measurement. This is it
      acqDimension = expDim
      break
  else:
    continue
  break
else:
  # Nothing found - should never happen. Take existing order as a default
  acqDimension = result[0]

if acqDimension is not result[0]:
  # Heuristic - given that most dimensions are given in traversal order for non-reversed
  if not self.isReversed:
    result.reverse()
  result = [acqDimension] + [x for x in result if x is not acqDimension]</item>
